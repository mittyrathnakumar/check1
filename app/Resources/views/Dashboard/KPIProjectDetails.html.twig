{% elseif KPIID == 15 or  KPIID == 16 %}
	{{ include ('Dashboard/ViewDocumentation.html.twig') }}	
{% elseif KPIID == 17 %}
	{{ include ('Dashboard/ViewP1P260.html.twig') }}
{% elseif KPIID == 18 %}
	{{ include ('Dashboard/ViewP3P460.html.twig') }}
{% elseif KPIID == 19 %}
	{{ include ('Dashboard/ViewP1P2365.html.twig') }}
{% elseif KPIID == 20 %}
	{{ include ('Dashboard/ViewP3P4365.html.twig') }}
{% elseif KPIID == 1 or  KPIID == 2 or KPIID == 3 or KPIID == 4 %}
	{{ include ('Dashboard/ViewQCKPI.html.twig') }}	
{% endif %}

<input type="hidden" id="KPIHidden" value="{{ KPIID }} ">

{% endblock %}

{% block javascripts %}
	{{ parent() }}
	<script src="{{ asset('js/Dashboard/Dashboard_common.js') }}"></script>		
	<script>
		DashboardGeneralFunctions();		
	</script>	
{% endblock %}

{% block documentready %}
	<script>			
		$.fn.editable.defaults.mode = 'inline';		
		
		$('[data-toggle="tooltip"]').tooltip({
			html: true,			
    		max-width:none;
		});
	</script>
{% endblock %}