	{{ include ('Dashboard/ViewP1P2KPI.html.twig') }}
{% elseif KPIID == 7 %}		
	{{ include ('Dashboard/ViewP3P4KPI.html.twig') }}
{% elseif KPIID == 8 %}		
	{{ include ('Dashboard/ViewTDCE.html.twig') }}
{% elseif KPIID == 9 %}		
	{{ include ('Dashboard/ViewAutomation.html.twig') }}	
{% elseif KPIID == 10 %}		
	{{ include ('Dashboard/ViewReusability.html.twig') }}	
{% elseif KPIID == 11 %}		
	{{ include ('Dashboard/ViewIntakeProcess.html.twig') }}	
{% elseif KPIID == 12 %}		
	{{ include ('Dashboard/ViewQualityEstimation.html.twig') }}
{% elseif KPIID == 13 %}		
	{{ include ('Dashboard/ViewSTAutoamtion.html.twig') }}	
{% elseif KPIID == 14 %}		
